    "lucide-react": "^0.555.0",
    "next": "16.0.4",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "node-cron": "^4.2.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.1",
    "sonner": "^2.0.7",
