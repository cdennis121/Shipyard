{
  "name": "shipyard",
  "version": "0.0.1-alpha",
  "description": "A self-hosted update server for Electron applications",
  "author": "Shipyard Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LunarForge/Shipyard.git"
  },
  "homepage": "https://github.com/LunarForge/Shipyard#readme",
  "bugs": {
    "url": "https://github.com/LunarForge/Shipyard/issues"
  },
  "keywords": [
    "electron",
    "update-server",
    "auto-updater",
    "electron-builder",
    "release-management",
    "self-hosted"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
